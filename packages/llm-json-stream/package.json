    "bun",
    "nodejs",
    "browser",
    "edge-runtime"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ComsIndeed/llm_json_stream_typescript.git"
  },
  "homepage": "https://github.com/ComsIndeed/llm_json_stream_typescript#readme",
  "bugs": {
    "url": "https://github.com/ComsIndeed/llm_json_stream_typescript/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "author": "Vincent San Nicolas",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
